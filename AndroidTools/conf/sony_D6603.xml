<?xml version="1.0" encoding="UTF-8"?>

<!--  Android Sony D6603 (Xperia Z3) configuration file   -->

<Android-config>

  <CanalPattern>
    <Pattern canal="keyboard" value="qpnp_pon"/>
    <Pattern canal="keyboard2" value="gpio-keys"/>-->
    <Pattern canal="touchscreen" value="clearpad"/>
  </CanalPattern>
  
  <KeyMapping canal="keyboard">
	<Key name="ON_OFF" code="116"/>
  </KeyMapping>
  
  <KeyMapping canal="keyboard2">
	<Key name="up" code="115"/>
	<Key name="down" code="114"/>
	<Key name="photo" code="528"/>
  </KeyMapping> -->

  <SoftKeyMapping/>
  
  <Touchscreen canal="touchscreen">
    <Pattern name="X" value="3 53 "/>
    <Pattern name="Y" value="3 54 "/>
    <Pattern name="ratioX" value="1.0"/>
    <Pattern name="ratioY" value="1.0"/>
    <Pattern name="down" value="3 58 0"/>
    <Pattern name="downmax" value="3 58 255"/>
    <Pattern name="up" value="3 58 1"/>
    <Pattern name="flush" value="0 0 0"/>
    <Pattern name="flush2" value="0 2 0"/>
    <Threshold name="move" value="15"/>
    <Option name="sendMouseDownForMove" value="true"/>
    <Option name="sendMouseEventFirst" value="true"/>
    <Option name="useMonkeyForPress" value="true"/>
	<Option name="sendFlushTwiceForMouseUp" value="true" />
  </Touchscreen>
  
</Android-config>
