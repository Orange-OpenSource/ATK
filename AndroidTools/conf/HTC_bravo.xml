<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<Android-config>     

       
	<KeyMapping>
    
        <Key name="VOLUME_UP" code="115" />
        <Key name="VOLUME_DOWN" code="114" />
        <Key name="BACK" code="158" />
        <Key name="HOME" code="102" />
        <Key name="MENU" code="139" />
        <Key name="SEARCH" code="217" />
        
        
	</KeyMapping>

	<CanalPattern>

        <Pattern canal="keyboard" value="keypad" />
       <!--  <Pattern canal="keyboard2" value="/dev/input/event3" />-->
        <Pattern canal="touchscreen" value="touchscreen" />
	</CanalPattern>
        
	<Touchscreen>
        <Pattern name="X" value="3 53 " />
        <Pattern name="Y" value="3 54 " />
        <Pattern name="down" value="3 48 1" />
        <Pattern name="downmax" value="3 48 255" />
        <Pattern name="up" value="3 48 0" />
		<Pattern name="flush" value="0 0 0" />
        <Pattern name="flush2" value="0 2 0" />
        <Pattern name="ratioX" value="8.4" />
        <Pattern name="ratioY" value="8.2" />
        <Threshold name="move" value="15" />
        <Option name="sendMouseEventFirst" value="true" />
		<Option name="sendMouseDownForMove" value="true" />
	</Touchscreen>

</Android-config>