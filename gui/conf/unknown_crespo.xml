<?xml version="1.0" encoding="UTF-8"?>

<!--  Android unknown crespo configuration file   -->
<!--   Screen resolution 480x800   -->
<Android-config>
  <CanalPattern>
    <Pattern canal="keyboard" value="herring-keypad"/>
    <Pattern canal="touchscreen" value="mxt224_ts_input"/>
  </CanalPattern>
  <KeyMapping canal="keyboard">
    <Key name="POWER" code="116"/>
    <Key name="VOLUME_UP" code="115"/>
    <Key name="VOLUME_DOWN" code="114"/>
  </KeyMapping>
  <SoftKeyMapping>
    <Key name="BACK" avgX="70" avgY="850"/>
    <Key name="HOME" avgX="425" avgY="850"/>
    <Key name="SEARCH" avgX="300" avgY="850"/>
  </SoftKeyMapping>
  <Touchscreen>
    <Pattern name="X" value="3 53 "/>
    <Pattern name="Y" value="3 54 "/>
    <Pattern name="ratioX" value="2.13"/>
    <Pattern name="ratioY" value="1.1875"/>
    <Pattern name="down" value="3 57 0"/>
    <Pattern name="downmax" value="3 57 4"/>
    <!--Pattern name="up" value="3 57 0"/-->
    <Pattern name="flush" value="0 0 0"/>
    <Pattern name="flush2" value="0 2 0"/>
    <Threshold name="move" value="15"/>
    <Option name="useSmartTouchDetection" value="true" />
    <Option name="sendFlushTwiceForMouseUp" value="true" />
    <Option name="useMonkeyForPress" value="true"/>
  </Touchscreen>
</Android-config>
