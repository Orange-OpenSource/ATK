<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<Android-config>
    <!-- Samsung Galaxy S I9000 Android v2.1 -->
	<KeyMapping canal="keyboard">
        <Key name="VOLUME_UP" code="42" />
        <Key name="VOLUME_DOWN" code="58" />
		<Key name="HOME" code="50" />
        <Key name="POWER" code="102" />
	</KeyMapping>
	
    <KeyMapping canal="keyboard2">
		<Key name="MENU" code="158" />
        <Key name="BACK" code="28" />
    </KeyMapping>
        


	<CanalPattern>

        <Pattern canal="keyboard" value="s3c-keypad" />
        <Pattern canal="keyboard2" value="melfas-touchkey" />
        <Pattern canal="touchscreen" value="qt602240_ts_input" />
	</CanalPattern>

	<Touchscreen>
        <Pattern name="Y" value="3 54 " />
        <Pattern name="X" value="3 53 " />
        <Pattern name="down" value="3 48 40" />
        <Pattern name="up" value="3 48 0" />
        <Pattern name="flush2" value="0 2 0" />
        <Pattern name="flush" value="0 0 0" />
        <Pattern name="ratio" value="1" />
		<Option name="sendMouseDownForMove" value="true" />
        <Threshold name="move" value="15" />
	</Touchscreen>

</Android-config>