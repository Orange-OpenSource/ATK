<?xml version="1.0" encoding="UTF-8"?>

<!--  Android HTC ace configuration file  = HTC Desire HD -->
<!--   Screen resolution 480x800 - Android v2.2  -->
<Android-config>
  <CanalPattern>
    <Pattern canal="keyboard2" value="spade-keypad-v0"/>
    <Pattern canal="touchscreen" value="synaptics-rmi-touchscreen"/>
  </CanalPattern>
  <KeyMapping canal="keyboard2">
    <Key name="POWER" code="116"/>
    <Key name="VOLUME_UP" code="115"/>
    <Key name="VOLUME_DOWN" code="114"/>
  </KeyMapping>
  <SoftKeyMapping>
    <Key name="HOME" avgX="100" avgY="840"/>
    <Key name="MENU" avgX="190" avgY="840"/>
    <Key name="SEARCH" avgX="415" avgY="840"/>
    <Key name="BACK" avgX="310" avgY="840"/>
  </SoftKeyMapping>
  <Touchscreen>
    <Pattern name="X" value="3 53 "/>
    <Pattern name="Y" value="3 54 "/>
    <Pattern name="ratioX" value="2.44"/>
    <Pattern name="ratioY" value="2.37"/>
    <Pattern name="down" value="3 48 1"/>
    <Pattern name="downmax" value="3 48 255"/>
    <Pattern name="up" value="3 48 0"/>
    <Pattern name="flush" value="0 0 0"/>
    <Pattern name="flush2" value="0 2 0"/>
    <Threshold name="move" value="15"/>
    <Option name="sendMouseDownForMove" value="true"/>
    <Option name="sendMouseEventFirst" value="true"/>
    <Option name="useMonkeyForPress" value="true"/>
  </Touchscreen>
</Android-config>
