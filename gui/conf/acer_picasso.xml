<?xml version="1.0" encoding="UTF-8"?>

<!--  Android Acer picasso configuration file   -->
<!--   Screen resolution 545x320   -->
<Android-config>
  <CanalPattern>
    <Pattern canal="keyboard" value="gpio-keys"/>
    <Pattern canal="touchscreen" value="atmel-maxtouch"/>
  </CanalPattern>
  <KeyMapping canal="keyboard">
    <Key name="POWER" code="116"/>
    <Key name="VOLUME_UP" code="115"/>
    <Key name="VOLUME_DOWN" code="114"/>
  </KeyMapping>
  <SoftKeyMapping/>
  <Touchscreen canal="touchscreen">
    <!--!!! THIS IS JUST TOUCHSCREEN Elements TEMPLATE !!!-->
    <!--!!! SEE ATK User Guide - Configuration section - and UPDATE following values !!!-->
    <!--!!! Please check following X Y ratioX and ratioY patterns-->
    <Pattern name="X" value="3 53 "/>
    <Pattern name="Y" value="3 54 "/>
    <Pattern name="ratio" value="1.0"/>
    <!--!!! Please update following patterns-->
    <Pattern name="down" value="3 48 1"/>
    <Pattern name="downmax" value="3 48 255"/>
    <Pattern name="up" value="3 48 0"/>
    <Pattern name="flush" value="0 0 0"/>
    <Pattern name="flush2" value="0 2 0"/>
    <Threshold name="move" value="10"/>
    <Option name="sendMouseDownForMove" value="true"/>
    <Option name="sendMouseEventFirst" value="true"/>
    <Option name="useMonkeyForPress" value="true"/>
	<Option name="sendFlushTwiceForMouseUp" value="true" />
	<Option name="useSmartTouchDetection" value="true" />
	<Option name="dontUseMonkey" value="true" />

  </Touchscreen>
</Android-config>
