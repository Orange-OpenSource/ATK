<?xml version="1.0" encoding="UTF-8"?>
<!--  LG G3 - Android version 4.4.2 -->
<!--   Screen resolution 1440 x 2560   -->
<Android-config>
  <CanalPattern>
      <Pattern canal="keyboard" value="gpio-keys"/>
      <Pattern canal="keyboard2" value="qpnp_pon"/>
    <Pattern canal="touchscreen" value="touch_dev"/>
  </CanalPattern>
  <KeyMapping canal="keyboard">
    <Key name="VOLUME_UP" code="115"/>
    <Key name="VOLUME_DOWN" code="114"/>
  </KeyMapping>
  <KeyMapping canal="keyboard2">
    <Key name="POWER" code="116"/>
  </KeyMapping>
  <SoftKeyMapping/>
  <Touchscreen>
    <Pattern name="X" value="3 53 "/>
    <Pattern name="Y" value="3 54 "/>
    <Pattern name="ratioX" value="1.0"/>
    <Pattern name="ratioY" value="1.0"/>
    <Pattern name="down" value="3 57 0"/>
    <Pattern name="downmax" value="3 57 65535"/>
    <Pattern name="up" value="3 57 -1"/>
    <Pattern name="flush" value="0 0 0"/>
    <Threshold name="move" value="150"/>
 <!--   <Option name="useSmartTouchDetection" value="true"/> -->
    <Option name="sendMouseEventFirst" value="true"/>
    <Option name="dontUseMonkey" value="true"/>
  </Touchscreen>
</Android-config>
