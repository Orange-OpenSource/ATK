<Android-config>
    <!-- Samsung Galaxy Spica Android v2.1 -->        
    <!-- Validated with ATK 2.10.4 -->
	<KeyMapping>
        <Key name="UP" code="202" />
        <Key name="DOWN" code="210" />
        <Key name="LEFT" code="218" />
        <Key name="RIGHT" code="203" />
        <Key name="CENTER" code="204" />
    
    
        <Key name="MENU" code="211" />
        <Key name="BACK" code="212" />
        <Key name="CALL" code="219" />
        <Key name="END_CALL" code="249" />
    
        <Key name="VOLUME_UP" code="201" />
        <Key name="VOLUME_DOWN" code="209" />
        <Key name="CAMERA" code="250" />
        
        <Key name="RETURN" code="204" />
        <Key name="HOME" code="227" />
        <Key name="WORLD" code="226" />
