<?xml version="1.0" encoding="UTF-8"?>

<!--  Android samsung GT-P7300 configuration file   -->
<!--   Screen resolution 1280x800   -->
<Android-config>
  <CanalPattern>
    <Pattern canal="keyboard" value="sec_key"/>
    <Pattern canal="keyboard2" value="sec_power_key"/>
    <Pattern canal="touchscreen" value="sec_touchscreen"/>
  </CanalPattern>
  <KeyMapping canal="keyboard">
    <Key name="VOLUME_UP" code="115"/>
    <Key name="VOLUME_DOWN" code="114"/>
  </KeyMapping>
  <KeyMapping canal="keyboard2">
    <Key name="POWER" code="116"/>
    <Key name="ALT_LEFT" code="116"/>
  </KeyMapping>
  <Touchscreen>
    <Pattern name="X" value="3 53 "/>
    <Pattern name="Y" value="3 54 "/>
    <Pattern name="ratio" value="1"/>
    <Pattern name="up" value="3 48 0"/>
    <Threshold name="move" value="15"/>
    <Option name="sendMouseDownForMove" value="true"/>
    <Option name="useSmartTouchDetection" value="true" />
    <Option name="useMonkeyForPress" value="true" />
  </Touchscreen>
</Android-config>
