<?xml version="1.0" encoding="UTF-8"?>

<!--  Android samsung GT-I9100 configuration file   -->
<!--   Screen resolution 480x800   -->
<Android-config>
  <CanalPattern>
    <Pattern canal="keyboard" value="gpio-keys"/>
    <Pattern canal="keyboard2" value="sec_touchkey"/>
    <Pattern canal="touchscreen" value="sec_touchscreen"/>
  </CanalPattern>
  <KeyMapping canal="keyboard">
    <Key name="HOME" code="102"/>
    <Key name="POWER" code="116"/>
    <Key name="VOLUME_UP" code="115"/>
    <Key name="VOLUME_DOWN" code="114"/>
  </KeyMapping>
  <KeyMapping canal="keyboard2">
    <Key name="MENU" code="139"/>
    <Key name="BACK" code="158"/>
  </KeyMapping>
  <SoftKeyMapping/>
  <Touchscreen>
    <Pattern name="X" value="3 53 "/>
    <Pattern name="Y" value="3 54 "/>
    <Pattern name="ratioX" value="1.0"/>
    <Pattern name="ratioY" value="1.0"/>
    <!--!!! Please update following patterns-->
    <Pattern name="down" value="3 57 1"/>
    <Pattern name="downmax" value="3 57 65535"/>
    <Pattern name="up" value="3 57 -1"/>
    <Pattern name="flush" value="0 0 0"/>
    <Threshold name="move" value="15"/>
    <Option name="sendMouseDownForMove" value="true"/>
  </Touchscreen>
</Android-config>
