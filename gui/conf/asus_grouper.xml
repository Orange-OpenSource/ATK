<?xml version="1.0" encoding="UTF-8"?>

<!--  Android asus grouper configuration file   -->
<!--   Screen resolution 1280x800 2111x1279   -->
<Android-config>
  <CanalPattern>
    <Pattern canal="keyboard" value="gpio-keys"/>
    <Pattern canal="touchscreen" value="elan-touchscreen"/>
  </CanalPattern>
  <KeyMapping canal="keyboard">
    <Key name="POWER" code="116"/>
    <Key name="VOLUME_UP" code="115"/>
    <Key name="VOLUME_DOWN" code="114"/>
  </KeyMapping>
  <SoftKeyMapping>
    <Key name="BACK" avgX="240" avgY="1300"/>
    <Key name="HOME" avgX="400" avgY="1300"/>
    <Key name="SEARCH" avgX="560" avgY="1300"/>
  </SoftKeyMapping>
  <Touchscreen>
    <Pattern name="X" value="3 53 "/>
    <Pattern name="Y" value="3 54 "/>
    <Pattern name="ratioX" value="1.65"/>
    <Pattern name="ratioY" value="1.6"/>
	<Pattern name="down" value="3 57 0"/>
	<Pattern name="downmax" value="3 57 65535"/>
	<Pattern name="up" value="3 57 -1"/>
    <Pattern name="flush" value="0 0 0"/>
    <Threshold name="move" value="30"/>
    <Option name="useMonkeyForPress" value="true"/>
  </Touchscreen>
</Android-config>
