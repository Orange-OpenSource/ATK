<?xml version="1.0" encoding="UTF-8"?>
<!-- MOTOROLA DEFY MB525 - Adnroid version 2.1-update1 -->
<!--  Android motorola umts_jordan configuration file   -->
<!--   Screen resolution 480x854   -->
<Android-config>
  <CanalPattern>
    <Pattern canal="keyboard" value="sholes-keypad"/>
    <Pattern canal="keyboard2" value="cpcap-key"/>
    <Pattern canal="touchscreen" value="qtouch-touchscreen"/>
  </CanalPattern>
  <KeyMapping canal="keyboard">
    <Key name="VOLUME_UP" code="115"/>
    <Key name="VOLUME_DOWN" code="114"/>
  </KeyMapping>
  <KeyMapping canal="keyboard2">
    <Key name="POWER" code="107"/>
  </KeyMapping>
  <SoftKeyMapping>
    <Key name="HOME" avgX="190" avgY="900"/>
    <Key name="MENU" avgX="60" avgY="900"/>
    <Key name="SEARCH" avgX="470" avgY="900"/>
    <Key name="BACK" avgX="320" avgY="900"/>
  </SoftKeyMapping>
  <Touchscreen>
    <!--!!! THIS IS JUST TOUCHSCREEN Elements TEMPLATE !!!-->
    <!--!!! SEE ATK User Guide - Configuration section - and UPDATE following values !!!-->
    <!--!!! Please check following X Y ratioX and ratioY patterns-->
    <Pattern name="X" value="3 53 "/>
    <Pattern name="Y" value="3 54 "/>
    <Pattern name="ratioX" value="2.08"/>
    <Pattern name="ratioY" value="1.1"/>
    <!--!!! Please update following patterns-->
    <Pattern name="down" value="3 48 1"/>
    <Pattern name="downmax" value="3 48 255"/>
    <Pattern name="up" value="3 48 0"/>
    <Pattern name="flush" value="0 0 0"/>
    <Pattern name="flush2" value="0 2 0"/>
    <Threshold name="move" value="15"/>
    <Option name="sendMouseDownForMove" value="true"/>
	<Option name="dontUseMonkey" value="true"/>
  </Touchscreen>
</Android-config>
