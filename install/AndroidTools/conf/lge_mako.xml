<?xml version="1.0" encoding="UTF-8"?>

<!--  Android LGE mako configuration file   -->
<!--   Screen resolution 320x494   -->
<Android-config>
  <CanalPattern>
    <Pattern canal="keyboard" value="keypad_8064"/>
    <Pattern canal="touchscreen" value="touch_dev"/>
  </CanalPattern>
  <KeyMapping canal="keyboard"/>
  <SoftKeyMapping/>
  <Touchscreen>
    <!--!!! THIS IS JUST TOUCHSCREEN Elements TEMPLATE !!!-->
    <!--!!! SEE ATK User Guide - Configuration section - and UPDATE following values !!!-->
    <Pattern name="X" value="0 0 "/>
    <Pattern name="Y" value="0 0 "/>
    <Pattern name="ratioX" value="1.0"/>
    <Pattern name="ratioY" value="1.0"/>
    <Pattern name="down" value="0 0 0"/>
    <Pattern name="downmax" value="0 0 0"/>
    <Pattern name="up" value="0 0 0"/>
    <Pattern name="flush" value="0 0 0"/>
    <Pattern name="flush2" value="0 2 0"/>
    <Threshold name="move" value="15"/>
    <Option name="sendMouseDownForMove" value="true"/>
    <Option name="sendMouseEventFirst" value="true"/>
    <Option name="useMonkeyForPress" value="true"/>
  </Touchscreen>
</Android-config>
