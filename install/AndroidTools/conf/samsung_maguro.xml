<?xml version="1.0" encoding="UTF-8"?>

<!--  Android samsung maguro (Galaxy Nexus) configuration file   -->
<!--   Screen resolution 720x1280   -->
<Android-config>
  <CanalPattern>
    <Pattern canal="keyboard" value="tuna-gpio-keypad"/>
    <Pattern canal="touchscreen" value="Melfas MMSxxx Touchscreen"/>
  </CanalPattern>
  <KeyMapping canal="keyboard">
    <Key name="POWER" code="116"/>
    <Key name="VOLUME_UP" code="115"/>
    <Key name="VOLUME_DOWN" code="114"/>
  </KeyMapping>
  <SoftKeyMapping>
    <Key name="BACK" avgX="170" avgY="1255"/>
    <Key name="HOME" avgX="360" avgY="1255"/>
    <Key name="SEARCH" avgX="580" avgY="1255"/>
  </SoftKeyMapping>
  <SoftKeyMapping/>
  <Touchscreen>
    <!--!!! THIS IS JUST TOUCHSCREEN Elements TEMPLATE !!!-->
    <!--!!! SEE ATK User Guide - Configuration section - and UPDATE following values !!!-->
    <Pattern name="X" value="3 53 "/>
    <Pattern name="Y" value="3 54 "/>
    <Pattern name="flush" value="0 0 0"/>
    <Pattern name="down" value="3 57 0"/>
    <Pattern name="downmax" value="3 57 65535"/>
    <Pattern name="up" value="3 57 -1"/>
    <Threshold name="move" value="15"/>
    <!--Option name="useSmartTouchDetection" value="true"/-->
    <Option name="useMonkeyForPress" value="true"/>
    <Option name="dontUseMonkey" value="true" />
  </Touchscreen>
</Android-config>
